package main.entities.ai;

public class AI {
	private int intelligence = 0;
	private int anger = 0;
	
	public static char move(){
		char command = 'v';
		int option = (int)(Math.random() * 4);
		switch(option){
			case 0: command = '^';
			break;
				  
			case 1: command = '>';
			break;
		  
			case 2: command = '<';
			break;
		  
			case 3: command = 'v';
			break;
		}
		

		return command;
	}
}
