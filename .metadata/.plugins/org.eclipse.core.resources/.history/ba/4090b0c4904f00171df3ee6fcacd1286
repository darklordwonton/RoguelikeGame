package main.entities.ai;

public class AI {
	protected int anger = 0;
	protected int bravery = 0;
	protected int inte = 0;
	
	public AI(){
		anger = 0;
		bravery = 0;
		inte = 0;
	}
	
	public AI(int a, int b, int i){
		anger = a;
		bravery = b;
		inte = i;
	}
	
	public static char turn (){
		char action = ' ';
		switch(inte){
		case 0: action = stupidMove();
			break;
		}
		
		
		return action;
	}
	
	public static char stupidMove(){
		
		
	}
}
