package main.dungeon;

import main.tiles.Tile;
import main.tiles.TileBlank;

public class Map {
	private Tile[][] tiles = null;
	private int height = 0;
	private int width = 0;	
	
	public Map (int h, int w) {
		height = h;
		width = w;
		tiles = new Tile[h][w];
		for (int i = 0; i < h; i++) {
			for (int e = 0; e < w; e++) {
				tiles[i][e] = new TileBlank();
			}
		}
	}
}
