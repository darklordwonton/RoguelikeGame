package main.util;

import main.dungeon.Floor;
import main.entities.Entity;
import main.entities.EntityPlayer;

public class Initializer {
	public static void start(){
		Globals.floors.add(new Floor(Globals.FLOOR_SIZE, Globals.FLOOR_SIZE));
		Globals.player = new EntityPlayer("Player", 10, 5, 5);
		Globals.floorss.get(0).addEntity(Globals.player);
	}
}
