package main.entities.ai;

public class AI {
	private int anger = 0;
	private int bravery = 0;
	private int inte = 0;
	
	public AI(){
		anger = 0;
		bravery = 0;
		inte = 0;
	}
	
	public AI(int a, int b, int i){
		anger = a;
		bravery = b;
		inte = i;
	}
	
	public static char turn (){
		char action = ' ';
		
		
		
		return action;
	}
}
